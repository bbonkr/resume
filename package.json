{
    "name": "resume",
    "version": "2.2.1",
    "description": "공개 이력서",
    "main": "docs/dist/app.js",
    "scripts": {
        "dev": "concurrently --kill-others \"npm run dev:netlify\" \"npm run dev:webpack\" ",
        "dev:netlify": "netlify dev",
        "dev:webpack": "webpack serve --config build/webpack.config.dev.js --mode development",
        "build:dev": "webpack --config build/webpack.config.dev.js",
        "build:prod": "webpack --config build/webpack.config.js",
        "build": "cross-env NODE_ENV=production npm run build:prod",
        "lint": "eslint --ext .ts,.tsx src/ --quiet --fix",
        "generate": "dotenv -e .env -- graphql-codegen",
        "test-api": "dotenv -e .env -- node .functions/resume.js"
    },
    "keywords": [
        "react",
        "typescript",
        "resume"
