{
    "extends": [
        "eslint:recommended",
        "next",
        "prettier"
    ],
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_",
                "vars": "all",
                "args": "none",
                "ignoreRestSiblings": false
            }
        ]
    }
}