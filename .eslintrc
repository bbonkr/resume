{
    "extends": [
        "eslint:recommended",
        "next",
        "prettier",
        "plugin:@next/next/recommended"
    ],
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern":"^_",
                "vars": "all",
                "args": "after-used",
                "ignoreRestSiblings": false
            }
        ]
    }
}